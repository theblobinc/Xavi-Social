<?xml version="1.0"?>
<schema version="0.3">
    <table name="XaviSocialAtprotoAccounts">
        <field name="id" type="I">
            <UNSIGNED/>
            <key/>
            <AUTOINCREMENT/>
        </field>
        <field name="userId" type="I">
            <UNSIGNED/>
            <notnull/>
        </field>
        <field name="did" type="C" size="255">
            <notnull/>
        </field>
        <field name="handle" type="C" size="255"/>
        <field name="issuer" type="C" size="255"/>
        <field name="pdsUrl" type="C" size="255"/>
        <field name="appviewUrl" type="C" size="255"/>
        <field name="scopes" type="X2"/>
        <field name="refreshToken" type="X2"/>
        <field name="accessToken" type="X2"/>
        <field name="accessTokenExpiresAt" type="I">
            <UNSIGNED/>
            <default value="0"/>
        </field>
        <field name="revoked" type="L">
            <UNSIGNED/>
            <default value="0"/>
            <notnull/>
        </field>
        <field name="createdAt" type="I">
            <UNSIGNED/>
            <default value="0"/>
        </field>
        <field name="updatedAt" type="I">
            <UNSIGNED/>
            <default value="0"/>
        </field>
    </table>
</schema>
